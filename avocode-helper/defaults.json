{
  "colors" : [
    {
      "value": "white",
      "name": "$color-white",
      "type": "color"
    },
    {
      "value": "black",
      "name": "$color-black",
      "type": "color"
    }
  ],
  "replaces": [
    {
      "value": "([0-9]+\\.?[0-9]*)px",
      "name": "em-auto($1px)",
      "type": "replace"
    }
  ],
  "font" : {
    "mixin" : "font-%s();"
  }
}